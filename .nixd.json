{
  "nixpkgs": {
    "expr": "import (builtins.getFlake \"/home/user/Nixos\").inputs.nixpkgs { }"
  },
  "formatting": {
    "command": ["alejandra"]
  },
  "options": {
    "nixos": {
      "expr": "(builtins.getFlake \"/home/user/Nixos\").nixosConfigurations.nixos-framework.options"
    },
    "home-manager": {
      "expr": "(builtins.getFlake \"/home/user/Nixos\").nixosConfigurations.nixos-framework.options.home-manager.users.type.getSubOptions []"
    }
  }
}
